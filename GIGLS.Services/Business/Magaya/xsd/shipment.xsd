<xs:element name="USPortOfInbond3" type="xs:string" minOccurs="0" maxOccurs="1"/>
<xs:element name="USPortOfInbond4" type="xs:string" minOccurs="0" maxOccurs="1"/>
<xs:element name="USPortOfInbond5" type="xs:string" minOccurs="0" maxOccurs="1"/>
